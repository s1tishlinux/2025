   "source": [
    "import itertools\n",
    "\n",
    "# Infinite iterator\n",
    "count = itertools.count(start=1, step=2)\n",
    "print(next(count))  # Output: 1\n",
    "print(next(count))  # Output: 3\n",
    "\n",
    "# Permutations\n",
    "items = ['A', 'B', 'C']\n",
    "for perm in itertools.permutations(items):\n",
    "    print(perm)"
   ]
  }
